{
  "name": "larva/laravel-flysystem-cos",
  "description": "This is a Flysystem adapter for the Tencent COS.",
  "keywords": [ "flysystem", "tencent", "cos", "larva" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "illuminate/support": "^6.0 || ^7.0 || ^8.0",
    "league/flysystem": "^1.0",
    "qcloud/cos-sdk-v5": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Flysystem\\Tencent\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larva\\Flysystem\\Tencent\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Larva\\Flysystem\\Tencent\\ObjectStorageServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
